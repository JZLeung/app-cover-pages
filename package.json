{
  "name": "cover-index",
  "version": "2.0.0",
  "description": "封面app网站首页",
  "main": "index.js",
  "author": "LeungJZ",
  "license": "MIT",
  "scripts": {
    "dev": "parcel index.html --no-cache --open",
    "dev:index": "parcel index.html --no-cache --open",
    "dev:mobile": "parcel mobile.html --no-cache --open",
    "build:index": "parcel build index.html -d build --detailed-report --no-cache",
    "build:mobile": "parcel build mobile.html -d build/mobile --public-url ./ --out-file index.html --detailed-report --no-cache",
    "build": "rm -rf build && npm run build:index"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "sass": "^1.23.7"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
